{
  "name": "exception-notify-ai-web",
  "private": true,
  "version": "0.2.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:opennextjs": "opennextjs-cloudflare build",
    "start": "next start",
    "lint": "next lint",
"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
"upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
"test:storage": "tsx lib/test-storage.ts",
"d1:migrate": "wrangler d1 execute exception-notify-db --file=./migrations/0001_create_code_map.sql",
"d1:migrate:local": "wrangler d1 execute exception-notify-db --local --file=./migrations/0001_create_code_map.sql",
"pre-deploy-check": "tsx scripts/pre-deploy-check.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.11.0",
    "highlight.js": "^11.11.1",
    "next": "^14.2.3",
    "pako": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "esbuild": "^0.21.5",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.3",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3",
    "wrangler": "^4.43.0"
  }
}
